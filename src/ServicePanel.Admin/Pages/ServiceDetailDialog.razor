<MudDialog>
    <DialogContent>
        <MudGrid>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.Address" Label="地址" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.ServiceName" Label="名称" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.Pid" Label="PID" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.DisplayName" Label="描述" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.Status" Label="状态" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.StartType" Label="启动类型" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.UpdateTime" Label="更新时间" ReadOnly="true" Format="yyyy/MM/dd HH:mm/ss" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.MemoryInfo" Label="内存" ReadOnly="true" />
            </MudItem>
            <MudItem xs="6">
                <MudTextField Value="@serviceModel.ThreadCount" Label="线程" ReadOnly="true" />
            </MudItem>
            <MudItem xs="12">
                <MudTextField Value="@serviceModel.ExePath" Label="路径名称" ReadOnly="true" />
            </MudItem>
            <MudItem xs="12">
                <MudTextField Value="@serviceModel.ImagePath" Label="命令行" ReadOnly="true" />
            </MudItem>
        </MudGrid>
    </DialogContent>
</MudDialog>
@code {
    [Parameter]
    public ServiceModel serviceModel{ get; set; }
}