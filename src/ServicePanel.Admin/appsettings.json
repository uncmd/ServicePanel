{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Orleans": "Information"
      }
    },
    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          {
            "Name": "Console"
          },
          {
            "Name": "File",
            "Args": {
              "path": "Logs/logs.txt",
              "rollingInterval": "Day"
            }
          }
        ]
      }
    },
    "Properties": {
      "Application": "ServicePanel.Admin"
    }
  }
}
